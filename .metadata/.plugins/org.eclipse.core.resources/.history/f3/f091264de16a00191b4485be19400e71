        function sample6_execDaumPostcode() {
            new daum.Postcode({
                oncomplete: function(data) {
                    // íììì ê²ìê²°ê³¼ í­ëª©ì í´ë¦­íìë ì¤íí  ì½ëë¥¼ ìì±íë ë¶ë¶.

                    // ê° ì£¼ìì ë¸ì¶ ê·ì¹ì ë°ë¼ ì£¼ìë¥¼ ì¡°í©íë¤.
                    // ë´ë ¤ì¤ë ë³ìê° ê°ì´ ìë ê²½ì°ì ê³µë°±('')ê°ì ê°ì§ë¯ë¡, ì´ë¥¼ ì°¸ê³ íì¬ ë¶ê¸° íë¤.
                    var addr = ''; // ì£¼ì ë³ì
                    var extraAddr = ''; // ì°¸ê³ í­ëª© ë³ì

                    //ì¬ì©ìê° ì íí ì£¼ì íìì ë°ë¼ í´ë¹ ì£¼ì ê°ì ê°ì ¸ì¨ë¤.
                    if (data.userSelectedType === 'R') { // ì¬ì©ìê° ëë¡ëª ì£¼ìë¥¼ ì ííì ê²½ì°
                        addr = data.roadAddress;
                    } else { // ì¬ì©ìê° ì§ë² ì£¼ìë¥¼ ì ííì ê²½ì°(J)
                        addr = data.jibunAddress;
                    }

                    // ì¬ì©ìê° ì íí ì£¼ìê° ëë¡ëª íìì¼ë ì°¸ê³ í­ëª©ì ì¡°í©íë¤.
                    if(data.userSelectedType === 'R'){
                        // ë²ì ëëªì´ ìì ê²½ì° ì¶ê°íë¤. (ë²ì ë¦¬ë ì ì¸)
                        // ë²ì ëì ê²½ì° ë§ì§ë§ ë¬¸ìê° "ë/ë¡/ê°"ë¡ ëëë¤.
                        if(data.bname !== '' && /[ë|ë¡|ê°]$/g.test(data.bname)){
                            extraAddr += data.bname;
                        }
                        // ê±´ë¬¼ëªì´ ìê³ , ê³µëì£¼íì¼ ê²½ì° ì¶ê°íë¤.
                        if(data.buildingName !== '' && data.apartment === 'Y'){
                            extraAddr += (extraAddr !== '' ? ', ' + data.buildingName : data.buildingName);
                        }
                        // íìí  ì°¸ê³ í­ëª©ì´ ìì ê²½ì°, ê´í¸ê¹ì§ ì¶ê°í ìµì¢ ë¬¸ìì´ì ë§ë ë¤.
                        if(extraAddr !== ''){
                            extraAddr = ' (' + extraAddr + ')';
                        }
                        // ì¡°í©ë ì°¸ê³ í­ëª©ì í´ë¹ íëì ë£ëë¤.
                        document.getElementById("sample6_extraAddress").value = extraAddr;
                    
                    } else {
                        document.getElementById("sample6_extraAddress").value = '';
                    }

                    // ì°í¸ë²í¸ì ì£¼ì ì ë³´ë¥¼ í´ë¹ íëì ë£ëë¤.
                    document.getElementById('sample6_postcode').value = data.zonecode;
                    document.getElementById("sample6_address").value = addr;
                    // ì»¤ìë¥¼ ìì¸ì£¼ì íëë¡ ì´ëíë¤.
                    document.getElementById("sample6_detailAddress").focus();
                }
            }).open();
        }
        </script>
        
        
